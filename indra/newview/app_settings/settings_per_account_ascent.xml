    </map>
    <key>AutoresponseNonFriendsItemID</key>
    <map>
      <key>Comment</key>
      <string>UUID of item to send along with AutoresponseNonFriends</string>
      <key>Persist</key>
      <integer>1</integer>
      <key>Type</key>
      <string>String</string>
      <key>Value</key>
