     bottom_delta="0"
     width="110">
      <button.commit_callback
          function="TopObjects.Profile" />
    </button>
    <button
     follows="bottom|left"
     height="20"
     label="Disable Selected"
     bottom_delta="-20"
     left="10"
     name="disable_selected_btn"
     width="110">
      <button.commit_callback
          function="TopObjects.DisableSelected" />
    </button>
    <button
     follows="bottom|left"
     height="20"
     label="Disable All"
     left_delta="110"
