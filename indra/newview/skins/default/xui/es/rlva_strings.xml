<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<llsd>
<map>

	<key>strings</key>
	<map>
		<!-- Primarily used when @showloc restricted -->
		<key>hidden_generic</key>
		<map>
			<key>value</key>
			<string>(Oculto)</string>
		</map>
		<key>hidden_parcel</key>
		<map>
			<key>value</key>
			<string>(Parcela Oculta)</string>
		</map>
		<key>hidden_region</key>
		<map>
			<key>value</key>
			<string>(Región oculta)</string>
		</map>
			
		<!-- Received/sent IMs will be replaced by the matching string when @recvim/sendim restricted -->
		<key>blocked_recvim</key>
		<map>
			<key>value</key>
			<string>*** MI bloqueado por tu visor</string>
			<key>description</key>
			<string>Se muestra en lugar del mensaje original cuando un MI entrante es bloqueado</string>
			<key>label</key>
			<string>MI Entrante bloqueado (local)</string>
			<key>customizable</key>
			<boolean>1</boolean>
		</map>
		<key>blocked_sendim</key>
		<map>
			<key>value</key>
			<string>*** MI bloqueado por el visor del remitente</string>
			<key>description</key>
			<string>Se muestra (y envía al participante remoto) cuando un IM saliente es bloqueado</string>
			<key>label</key>
			<string>IM Saliente bloqueado (local + remoto)</string>
			<key>customizable</key>
			<boolean>1</boolean>
		</map>
			
		<!-- Shown as notifications -->
		<key>blocked_generic</key>
		<map>
			<key>value</key>
			<string>No se puede realizar la acción debido a las restricciones RLV</string>
		</map>
		<key>blocked_permattach</key>
		<map>
			<key>value</key>
			<string>El intento de anexar el '[OBJECT]' fué denegado debido a las restricciones RLV</string>
		</map>
		<key>blocked_permteleport</key>
		<map>
			<key>value</key>
			<string>al [OBJECT]' le fue denegado el permiso para teleportarte debido a las restricciones RLV</string>
		</map>
		<key>blocked_startim</key>
		<map>
			<key>value</key>
			<string>No se puede iniciar sesión de MI con [RECIPIENT] debido a las restricciones RLV</string>
		</map>
		<key>blocked_startconf</key>
		<map>
			<key>value</key>			
			<string>No se puede iniciar una conferencia debido a las restricciones RLV</string>
		</map>
		<key>blocked_teleport</key>
		<map>
			<key>value</key>
			<string>No se puede iniciar el Teleporte debido a las restricciones RLV</string>
		</map>
		<key>blocked_viewxxx</key>
		<map>
			<key>value</key>
			<string>No se puede abrir [TYPE] debido a las restricciones RLV</string>
		</map>
		<key>blocked_wireframe</key>
		<map>
			<key>value</key>
			<string>No se puede habilitar el modo wireframe debido a las restricciones RLV</string>
		</map>

		<!-- Sent as "Busy" messages to the remote party -->
		<key>blocked_recvim_remote</key>
		<map>
			<key>value</key>
			<string>El residente al que le has enviado el mensaje está impedido momentáneamente de leer tus Mensajes Instantáneos, por favor, inténtalo más tarde.</string>
			<key>description</key>
			<string>Se envía al participante remoto cuando su IM fue bloqueado</string>
			<key>label</key>
			<string>MI entrante bloqueado (remoto)</string>
			<key>customizable</key>
			<boolean>1</boolean>
		</map>
		<key>blocked_tplurerequest_remote</key>
		<map>
			<key>value</key>
			<string>El residente al que has invitado está impedido de aceptar. Por favor, inténtalo más tarde.</string>
			<key>description</key>
			<string>Se envía al participante remoto cuando su oferta o petición de teleporte fue bloqueada</string>
			<key>label</key>
			<string>Oferta/Petición de Teleporte bloqueada (remoto)</string>
			<key>customizable</key>
			<boolean>1</boolean>
		</map>
	</map>
		
