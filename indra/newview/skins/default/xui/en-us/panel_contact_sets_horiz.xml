<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel border="true" bottom="-371" height="300" left="300" mouse_opaque="true"
     name="contact_sets" width="350">
	<scroll_list background_visible="true" bottom="109" column_padding="0" draw_border="true"
	     draw_heading="true" draw_stripes="true" enabled="true"
	     follows="left|top|right|bottom" left="5" mouse_opaque="true"
	     multi_select="false" name="contact set list" tab_stop="true" top="-10"
	     width="340" menu_num="1">
		<column dynamicwidth="true" label="Name" name="contactsetname" tool_tip="Name" />
		<column width="0" label="ID" name="contactsetid" tool_tip="ID" />
    
	</scroll_list>
	

	<button bottom_delta="-24" follows="left|bottom" font="SansSerif" height="22"
	     label="Add" left_delta="0" name="Add"
	     tool_tip="Add a contact set" width="80">
         <button.commit_callback parameter="ContactSet.Add"/>
    </button>     
    
	<button bottom_delta="0" follows="left|bottom" font="SansSerif" height="22"
	     label="Edit" left_delta="82" name="Edit"
	     tool_tip="Edit a contact set" width="80" />	 
	<button bottom_delta="0" follows="left|bottom" font="SansSerif" height="22"
	     label="Delete" left_delta="82" name="Delete"
	     tool_tip="Delete a contact set" width="80" />

	
</panel>
